<resources>
    <!-- Base application theme. -->
    <style name="Theme.UAS_10120905_JohndyPanca" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/ebony</item>
        <item name="colorPrimaryVariant">@color/ebony</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.UAS_10120905_JohndyPanca.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ThemeOverlay.AppCompat.navTheme">
        <item name="colorPrimary">@color/valencia</item>
        <item name="colorControlHighlight">@color/valencia</item>
    </style>

    <style name="NavbarStyle" parent="android:Widget.TextView">
        <item name="android:fontFamily"> @font/monumentextended_regular</item>
    </style>


</resources>